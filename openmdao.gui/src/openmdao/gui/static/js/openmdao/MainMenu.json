[
    { "text": "Project", "id": "project-menu", 
      "items": [
        { "text": "Save",          "id": "project-save", "onclick": "openmdao.model.saveProject();" },
        { "text": "Run",           "id": "project-run", "onclick": "openmdao.model.runModel();" },
        { "text": "Reload",        "id": "project-reload", "onclick": "openmdao.model.reloadProject();" },
        { "text": "Close",         "id": "project-close", "onclick": "openmdao.model.closeProject();" },
        { "text": "Exit",          "id": "project-exit", "onclick": "openmdao.model.exitProject();" }
      ]
    },
    { "text": "View", "id": "view-menu",
      "items": [
        { "text": "Code Editor",   "id": "view-code", "onclick": "jQuery('#code_tab').click();" },
        { "text": "Command Line",  "id": "view-cmdline", "onclick": "openmdao.Util.toggle_visibility('cmdform');" },
        { "text": "Console",       "id": "view-console", "onclick": "layout.open('south');" },
        { "text": "Files",         "id": "view-files", "onclick": "layout.open('west'); jQuery('#ftree_tab').click();" },
        { "text": "Libraries",     "id": "view-libraries", "onclick": "layout.open('east'); jQuery('#palette_tab').click();" },
        { "text": "Objects",       "id": "view-objects", "onclick": "layout.open('west'); jQuery('#otree_tab').click();" },
        { "text": "Properties",    "id": "view-properties", "onclick": "layout.open('east'); jQuery('#properties_tab').click();" },
        { "text": "Workflow",      "id": "view-workflow", "onclick": "jQuery('#workflow_tab').click();" },
        { "text": "Structure",     "id": "view-structure", "onclick": "jQuery('#structure_tab').click();" },
        { "text": "--------"       },
        { "text": "Refresh",       "id": "view-refresh", "onclick": "openmdao.model.refresh();" }
      ]
    },
    { "text": "Tools", "id": "tools-menu",
      "items": [
        { "text": "Plotter",       "id": "tools-plotter", "onclick": "new openmdao.Plotter()"},
        { "text": "Plugins",       "id": "tools-plugins", "onclick": "openmdao.Util.popupWindow('addons','Addons',575,540)"},
        { "text": "3DTin",         "id": "tools-3dtin", "onclick": "openmdao.Util.popupWindow('http://www.3dtin.com','3DTin',768,1024)"}
      ]
    },
    { "text": "Help", "id": "help-menu",
      "items": [
        { "text": "Documentation", "id": "help-doc", "onclick": "openmdao.Util.popupWindow('http://openmdao.org/docs','',768,1024);"}
      ]
    },
    { "text": "About",             "id": "about-item", "onclick": "openmdao.Util.popupWindow('http://openmdao.org/','',768,1024);"}
]
